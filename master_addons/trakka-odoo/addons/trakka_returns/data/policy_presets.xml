    <field name="code">standard</field>
    <field name="handling_fee_min">80</field>
    <field name="handling_fee_max">120</field>
  </record>
  <record id="policy_friendly" model="trakka.policy">
    <field name="name">Friendly</field>
    <field name="code">friendly</field>
    <field name="handling_fee_min">0</field>
