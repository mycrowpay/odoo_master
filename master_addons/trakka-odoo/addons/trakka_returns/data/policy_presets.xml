  </record>
  <record id="policy_strict" model="trakka.policy">
    <field name="name">Strict</field>
    <field name="code">strict</field>
    <field name="handling_fee_min">120</field>
    <field name="handling_fee_max">150</field>
  </record>
</odoo>
