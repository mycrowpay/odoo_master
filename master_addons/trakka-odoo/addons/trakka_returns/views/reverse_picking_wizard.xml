<odoo>
  <record id="view_trakka_reverse_picking_wizard" model="ir.ui.view">
    <field name="name">trakka.reverse.picking.wizard.form</field>
    <field name="model">trakka.reverse.picking.wizard</field>
    <field name="arch" type="xml">
      <form string="Create Reverse Picking">
        <group>
          <field name="return_case_id" readonly="1"/>
          <field name="picking_id" readonly="1"/>
          <field name="include_all"/>
        </group>
        <footer>
          <button string="Create Reverse" type="object" name="action_create_reverse" class="oe_highlight"/>
          <button string="Cancel" class="btn-secondary" special="cancel"/>
        </footer>
      </form>
    </field>
  </record>
</odoo>
